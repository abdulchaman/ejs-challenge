<%- include("partials/header"); -%>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
      <label for="titleExample">Title</label>
      <input class="form-control" type="text" name="postTitle" id="titleExample">
  </div>
  <div class="form-group">
     <label for="postExample">Post</label>  
     <textarea class="form-control" name="postBody" id="postExample"  rows="5"></textarea> 
  </div>
  <button class="btn btn-lg btn-primary" type="submit">Publish</button>
</form>

<%- include("partials/footer"); -%>